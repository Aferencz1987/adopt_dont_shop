<h1>New Application</h1>
<%= form_with url: "/pet_applications", method: :post, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, value: @pet_application.name %><br><br>

  <%= f.label :street %>
  <%= f.text_field :street, value: @pet_application.street %><br><br>

  <%= f.label :city %>
  <%= f.text_field :city, value: @pet_application.city%><br><br>

  <%= f.label :state %>
  <%= f.text_field :state, value: @pet_application.state %><br><br>

  <%= f.label :zip %>
  <%= f.text_field :zip, zip: @pet_application.zip %><br><br>

  <%= f.submit %>

<% end %>
<br>
